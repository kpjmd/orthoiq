{
  "functions": {
    "app/api/claude/route.ts": {
      "maxDuration": 120
    },
    "app/frames/route.tsx": {
      "maxDuration": 30
    },
    "app/api/health/route.ts": {
      "maxDuration": 30
    },
    "app/api/health/database/route.ts": {
      "maxDuration": 30
    },
    "app/api/health/claude/route.ts": {
      "maxDuration": 30
    },
    "app/api/webhooks/farcaster/route.ts": {
      "maxDuration": 30
    },
    "app/api/notifications/reset-daily/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/cleanup-sessions/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/send-milestone-notifications/route.ts": {
      "maxDuration": 120
    }
  },
  "crons": [
    {
      "path": "/api/notifications/reset-daily",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/send-milestone-notifications",
      "schedule": "0 9 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/.well-known/farcaster.json",
      "destination": "/api/farcaster-manifest"
    }
  ]
}